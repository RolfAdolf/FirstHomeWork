1)
x = linspace(-10, 10, 150)
y = x + 1./(x.^2)
plot(x, y)




2)
x = linspace(-10, 10, 150)
y = (2.*x)./(1+(x.^2))
plot(x, y)



7)
x = linspace(-10, 10, 150)
y = x.*sin(pi.*x);
plot(x, y)


11)
t = linspace(-5, 5, 300)
a = 1/2;
b = 1/3;
m = 7;
n=17;
x = cos(t)+a.*cos(m.*t) + b.*sin(n.*t)
y = sin(t)+a.*sin(m.*t) + b.*cos(n.*t)
plot(x, y)

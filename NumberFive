1)Лемниската Бернулли
   c = 1;
   F = @(x,y)(x.^2 + y.^2).^2 - (2*(c^2))*(x.^2 - y.^2);
   A = ezplot(F, [-2 2 -2 2]);
   grid on


2)Кардиоида
   r = 1;
   F = @(x,y)(x.^2 + y.^2 - 2*r*x).^2 - 4*(r^2)*(x.^2 + y.^2);
   A = ezplot(F, [-8 8 -8 8]);
   grid on

3)Полукубическая парабола
   F = @(x,y)(y.^2) - (x.^3);
   A = ezplot(F, [-2 2 -2 2]);
   grid on
